    "sourceMap": true,
    "noImplicit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true

  },
  "exclude": [
    "node_modules"
  ]
}